    <Company>Cloudmob Desenvolvimento e Consultoria Ltda</Company>
    <PackageTags>minio;CoreComponents;Helper</PackageTags>
    <PackageProjectUrl>https://github.com/CoreComponents/MinioHelper</PackageProjectUrl>
